      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Fake.Core.Trace" version="5.20.0" exclude="Build,Analyzers" />
        <dependency id="FSharp.Core" version="4.7.1" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="netstandard" targetFramework=".NETFramework4.7.2" />
    </frameworkAssemblies>
  </metadata>
